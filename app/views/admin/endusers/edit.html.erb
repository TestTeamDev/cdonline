<header>
</header>
<div class="main-contents">
  <h2>ユーザー情報編集</h2>
  <%= form_for @user,url: edit_admin_enduser_path do |f| %>
    <div class="form-group has-icon">
      <label class="control-label sr-only" for="inputFirst_name">苗字</label>
      <%= f.text_field :first_name,autofocus: :true,class:"form-control",id: "inputFirst_name",placeholder:"苗字" %>
      <%= f.submit "Update user", class:"btn btndefault" %>
    </div>
  <% end %>
</div>