<header>
</header>
<div class="maincontents">
  <h3 class="title">ユーザー一覧</h3>
<div class="col-xs-12">
  <table class="table table-striped">
  	<thead>
  	 <tr>
  	 	<th>user id</th>
  	 	<th>user name</th>
  	 	<th>email</th>
  	 </tr>
  	</thead>
  	<% @users.each do |user| %>
  	<tbody>
  	 <tr>
  	   <th><%= user.id %></th>
  	   <th><%= link_to user.name, enduser_enduser_path(user.id) %></th>
  	   <th><%= user.email %></th>
  	 </tr>
  	</tbody>
  	<% end %>
  </table>
</div>
  </div>